<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
  </configSections>

  <spring>
    <context>
      <resource uri="config://spring/objects" />
    </context>
    <objects xmlns="http://www.springframework.net">
      <object id="MainWindowViewModel" type="GetHandleApp.ViewModel.MainWindowViewModel" init-method="Initialize">
        <property name="Model" ref="GetHandleModel" />
      </object>
      <object id="GetHandleModel" type="GetHandleApp.Model.GetHandleModel">
        <property name="WindowProcFactory" ref="WindowProcFactory" />
      </object>
      <object id="WindowProcFactory" type="WindowHandleImplement.WindowProcFactoryImpl, WindowHandleImplement" />
    </objects>
  </spring>

</configuration>
